{
  "metadata": [
    {
      "src": [
        {
          "src": "../../Assets/",
          "files": [
              "**.cs"
          ]
        },
        {
          "src": "../../com.playeveryware.eos/"
        }
      ],
      "dest": "api",
      "filter": "filter.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": true,
      "ignoreErrors":true,
      "allowCompilationErrors": true,
      "properties": {
        "DefineConstants":"EOS_DISABLE;EOS_DYNAMIC_BINDINGS"
      }
    }
  ],
  "allowCompilationErrors":true,
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
